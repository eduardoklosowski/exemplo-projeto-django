{
  "name": "ExemploProjetoDjango",
  "dockerComposeFile": "docker-compose.yml",
  "service": "dev",
  "postCreateCommand": "./scripts/configure-env.sh",
  "forwardPorts": [8000, 5432],
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "customizations": {
    "vscode": {
      "extensions": [
        "EditorConfig.EditorConfig",
        "bungcip.better-toml",
        "ms-python.python",
        "charliermarsh.ruff"
      ]
    }
  }
}
